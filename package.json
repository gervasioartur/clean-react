{
  "name": "clean-react",
  "version": "1.2.0",
  "main": "index.js",
  "description": "React project using TDD, Clean arch and Typescript",
  "author": "Gervasio Artur Dombo gervasioartur@outlook.com.br",
  "license": "GNU GPL3",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint .",
    "lint:fix": "yarn lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test -- --watch",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "prepare": "husky install",
    "check": "yarn-check -u -s"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@testing-library/react": "^12.1.0",
    "@types/axios": "^0.14.0",
    "@types/faker": "^6.6.9",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.11.4",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "clean-webpack-plugin": "^4.0.0",
    "coveralls": "3.1.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "11",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^6.6.6",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "jest-localstorage-mock": "^2.4.3",
    "lint-staged": "^13.0.3",
    "node-sass": "^7.0.3",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "install": "^0.13.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
